	<script type="text/javascript">
	    $(window).ready(function () {
	        var asyncslider = $(".asyncslider_container");
	        asyncslider.asyncSlider({
	            direction: 'horizontal',
	            prevNextNav: false,
	            centerPrevNextNav: -60,
	            slidesNav: $("#slider_buttons_env"),
	            minTime: 800,
	            maxTime: 1200,
	            easing: 'easeInOutQuad',
	            keyboardNavigate: true,
	            random: true,
	            autoswitch: 5000
	        });
	        $(".slideup_vertically").click(function (ev) {
	            ev.preventDefault();

	            asyncslider.asyncSlider("set", "callback", function () {
	                asyncslider.asyncSlider("set", "direction", "horizontal");
	            });

	            asyncslider.asyncSlider("set", "direction", "vertical");
	            asyncslider.asyncSlider("next", "top");
	        });

	        $(".slidenext").click(function (ev) {
	            ev.preventDefault();
	            asyncslider.asyncSlider("next", "right");
	        });

	        /* Version 1.1 */
	        // Auto Switcher
	        $(".auto_switcher").click(function (ev) {
	            ev.preventDefault();

	            var status_span = $(this).find('span');

	            var current_status = status_span.hasClass('on') ? 1 : 0;

	            if (current_status == 1) // Is ON
	            {
	                status_span.removeClass('on').addClass('off').text('');

	                // Turn OFF AutoSwitcher
	                asyncslider.asyncSlider("set", "autoswitch", "off");
	            }
	            else {
	                status_span.removeClass('off').addClass('on').text('');

	                // Turn ON AutoSwitcher
	                asyncslider.asyncSlider("set", "autoswitch", "on");
	            }
	        });
	    });
</script>

  
